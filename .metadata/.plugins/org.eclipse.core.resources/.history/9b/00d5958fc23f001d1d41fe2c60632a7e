import java.util.Scanner;

public class App {

	public static void main(String[] args) {
		
		Person obj = new Person();
		obj.setName("Jonas");
		obj.setAge(20);
		
		Person obj2 = new Person();
		obj2.setName("Antanas");
		obj2.sayHi();
		
		System.out.println(obj.getName());
		System.out.println(obj2.getName());
	}	

}
