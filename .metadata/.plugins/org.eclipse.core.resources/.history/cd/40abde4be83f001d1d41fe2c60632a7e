
public class KetvirtaPaskaitaUzduotys {
	
	static String cleanUp(String in) {
		
		return in.replaceAll("\\s+", " ").trim();

	}
	static String cleanUp2(String in) {
		
		in = in.trim();
		while(hasTwoSpaces(in)) {
			in = in.replace("  " , " ");
		}
		return in;

	}
	static boolean hasTwoSpaces(String str) {
		 return str.contains("  ");
	}

}
