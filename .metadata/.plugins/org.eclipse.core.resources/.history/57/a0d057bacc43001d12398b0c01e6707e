
public enum Diena {

	PIRMADIENIS(false,1),
	ANTRADIENIS(false,2),
	TRECIADIENIS(false,3),
	KETVIRTADIENIS(false,4),
	PENKTADIENIS(false,5),
	SESTADIENIS(true,6),
	SEKMADIENIS(true,7);
	
	final boolean savaitgalis;
	final int kelintaDiena;
	
	Diena(boolean laisva, int nr){
		savaitgalis = laisva;
		kelintaDiena = nr;
	}
	
}
