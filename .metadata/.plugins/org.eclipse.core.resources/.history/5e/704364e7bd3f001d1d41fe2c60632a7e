
public class Calc {
	
	private int someInt = 6;
	
	private static int someStaticInt = 8;
	
	public void greet() {
		System.out.println("Hello World!!!");
	}
	
	public static int sum(int a, int b) {
		return a + b + someStaticInt;
	}
	public int sudeti(int a, int b) {
		return a + b + someInt;
	}


}
