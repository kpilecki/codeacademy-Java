public class Program {

	public static void main(String[] args) {

		//SeptintaPaskaita.printMedziaga();
		
		float metersToConvert = 5.0f;
		
		UOM testUOM = UOM.CENTIMETER;
		
		System.out.println(UOM.KILOMETER.convertAndToString(metersToConvert));
		
		System.out.println(UOM.METER.convertAndToString(metersToConvert));
		
		System.out.println(UOM.DECIMETER.convertAndToString(metersToConvert));
		
		System.out.println(testUOM.convertAndToString(metersToConvert));
		
		System.out.println(UOM.MILIMETER.convertAndToString(metersToConvert));
		

	}

}
