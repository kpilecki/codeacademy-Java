
public class TreciaPaskaita {
	
	public static void loops() {
		
		// Atspaudinti skaiciu nuo 0 iki 5
		
		int i = 0;
		
		while(i <= 5) {
			System.out.println(i++);
		}
		while(i > 1) {
			i--;
			System.out.println("Labas Rytas");
		}
		
		i=0;
		
		while(i <= 20) {
			System.out.println(i+=2);
		}
		
		
	}

}
