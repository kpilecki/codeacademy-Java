
public class TreciaPaskaita {
	
	public static void whileLoop() {
		
		// Atspaudinti skaiciu nuo 0 iki 5
		
		int i = 0;
		
		while(i <= 5) {
			System.out.println(i++);
		}
		while(i > 1) {
			i--;
			System.out.println("Labas Rytas");
		}
		
		i=0;
		
		while(i <= 20) {
			System.out.println( i );
			i +=2;
		}
		
		i=0;
		
		do {
			if( i % 2 == 0) {
			System.out.println( i );
			}
			i++;
		}
		while( i <= 20);
		
		
	}

}
